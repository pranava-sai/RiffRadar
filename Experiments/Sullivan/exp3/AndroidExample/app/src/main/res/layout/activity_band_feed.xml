            android:layout_height="60dp"
            android:layout_gravity="right"
            android:layout_marginEnd="20dp"
            android:scaleType="fitXY"
            android:contentDescription="venue"
            app:srcCompat="@android:drawable/ic_menu_mapmode" />
    </LinearLayout>

